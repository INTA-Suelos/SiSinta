= paginator.render do
  %nav
    %ul.pagination.pagination-sm.justify-content-end
      = first_page_tag
      = prev_page_tag
      - each_page do |page|
        - if page.left_outer? || page.right_outer? || page.inside_window?
          = page_tag page
        - elsif !page.was_truncated?
          = gap_tag
      = next_page_tag
      = last_page_tag

    -# FIXME Ver cómo ponerlo en la misma línea
    %ul.elegir_filas.pagination.pagination-sm.justify-content-end
      -# Botones para elegir filas según controller
      = elegir_filas params[:controller]
