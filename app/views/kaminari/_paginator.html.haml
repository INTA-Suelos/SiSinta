= paginator.render do
  %nav.row.justify-content-end
    %ul.elegir-paginas.pagination.pagination-sm
      = first_page_tag
      = prev_page_tag
      - each_page do |page|
        - if page.left_outer? || page.right_outer? || page.inside_window?
          = page_tag page
        - elsif !page.was_truncated?
          = gap_tag
      = next_page_tag
      = last_page_tag

    %ul.elegir-filas.pagination.pagination-sm
      -# Botones para elegir filas seg√∫n controller
      = elegir_filas params[:controller]
